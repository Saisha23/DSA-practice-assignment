1. heighest frequency character

class Solution {
  public:
    char getMaxOccuringChar(string& s) {
        unordered_map<char,int> mpp;
        for (char i: s){
            mpp[i]++;
        }
        int maxi=0;
        char ans=' ';
        for (auto it: mpp){
            if (maxi<it.second){
                maxi=it.second;
                ans=it.first;
            }
            else if (it.second == maxi && it.first < ans) {
                ans = it.first;}
        }
        return ans;
        
    }
};
